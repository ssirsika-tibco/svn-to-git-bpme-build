<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox spacing="5.0" xmlns="http://javafx.com/javafx/8.0.72" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tibco.bpm.acecannon.controllers.GenericJsonController">
   <children>
      <Label fx:id="labelTitle" text="JSON View">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <TabPane fx:id="tpFormat" prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
        <tabs>
          <Tab text="Raw">
               <content>
                  <TextArea fx:id="taRaw" editable="false" prefHeight="200.0" prefWidth="200.0" />
               </content>
          </Tab>
          <Tab text="Tree">
               <content>
                  <TreeView fx:id="tvTree" prefHeight="200.0" prefWidth="200.0" />
               </content>
          </Tab>
        </tabs>
      </TabPane>
   </children>
   <padding>
      <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
   </padding>
</VBox>
